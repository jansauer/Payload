{
  "name": "jansauer",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "generate:importmap": "NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src",
    "lint:fix": "npm run lint:fix:prettier && npm run lint:fix:eslint",
    "lint:fix:prettier": "prettier --write .",
    "lint:fix:eslint": "eslint src --fix",
    "start": "NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.58.0",
    "@payloadcms/next": "3.58.0",
    "@payloadcms/richtext-lexical": "3.58.0",
    "@payloadcms/storage-gcs": "3.58.0",
    "graphql": "16.11.0",
    "nanoid": "5.1.6",
    "next": "15.5.4",
    "payload": "3.58.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@types/node": "24.6.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.4",
    "prettier": "3.6.2",
    "typescript": "5.9.3"
  }
}
