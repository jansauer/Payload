{
  "name": "jansauer",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "generate:importmap": "NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint src",
    "lint:fix": "npm run lint:fix:prettier && npm run lint:fix:eslint",
    "lint:fix:prettier": "prettier --write .",
    "lint:fix:eslint": "eslint src --fix",
    "test": "vitest run --coverage",
    "start": "NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.64.0",
    "@payloadcms/next": "3.64.0",
    "@payloadcms/richtext-lexical": "3.64.0",
    "@payloadcms/storage-gcs": "3.64.0",
    "cheerio": "1.1.2",
    "feed": "5.1.0",
    "graphql": "16.12.0",
    "nanoid": "5.1.6",
    "next": "16.0.3",
    "payload": "3.64.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "4.0.8",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.3",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "vitest": "4.0.8"
  }
}
